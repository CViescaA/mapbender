{% for item in items %}
  <li class="navItem{% if item.active %} activeItem{% endif %}">
    <a href="{{ path(item.route) }}"><i class="fa fas fa-caret-right"></i>&nbsp;{{ item.title|trans }}</a>
  </li>
    {%- set items = item.subroutes  -%}
    {%- if items|length -%}
      <ul class="navLevel2">
          {{ include('MapbenderManagerBundle:Index:menu-items.html.twig') }}
      </ul>
    {%- endif -%}
{% endfor %}
