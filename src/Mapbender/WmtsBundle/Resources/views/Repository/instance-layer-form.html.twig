<tr class="item level0" >
    <td class="level1 itemType"><i class="itemType fa fas fa-caret-right"></i></td>
    <td class="titleColumn" title="{{ form.title.vars.label | trans }}">{{ form_widget(form.title) }}</td>
    <td title="{{ form.tileMatrixSet.vars.label | trans }}">{{ form_widget(form.tileMatrixSet) }}</td>
    {{ form_row(form.active) }}
    {{ form_row(form.allowselected) }}
    {{ form_row(form.selected) }}
    {{ form_row(form.allowinfo) }}
    {{ form_row(form.info) }}
    <td class="moreColumn">
    <div class="static-popover-wrap">
      <div class="infoMsgBox popover left hidden">
        <div class="arrow"></div>
        <div class="popover-content">
          <div>
            <span class="labelText">{{ "mb.wms.wmsloader.repo.instancelayerform.label.id.title"|trans}}</span><span class="smallText" title="{{ "mb.wms.wmsloader.repo.instancelayerform.label.id.description"|trans}}">{{form.vars.data.sourceInstance.source.id}}-{{form.vars.data.sourceItem.id}}/{{form.vars.data.sourceInstance.id}}-{{form.vars.data.id}}</span>
          </div>
          <div>
              <span class="labelText">{{ "mb.wms.wmsloader.repo.instancelayerform.label.layersname"|trans }}</span><span class="smallText">{{ form.vars.data.sourceItem.identifier }}</span>
          </div>
        {{ form_row(form.style) }}
        </div>
      </div>
      <span class="-fn-toggle-layer-detail" title="{{'mb.wms.wmsloader.repo.instancelayerform.label.moreinfo'|trans}}"><i class="fa fas fa-ellipsis-h"></i></span>
      </div>
    </td>
</tr>
