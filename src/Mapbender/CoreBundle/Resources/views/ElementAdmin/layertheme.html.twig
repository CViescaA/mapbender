<tr class="collectionItem">
    <td title="{{"mb.core.admin.layertree.label.theme.label"|trans}}">
        {{ theme.vars.data.title }}
    </td>
    <td class="theme-settings text-nowrap text-right">
        <span title="{{"mb.core.admin.layertree.label.theme.useTheme"|trans}}">
            {{ form_widget(theme.useTheme) }}
        </span>
        <span class="item-opened" title="{{"mb.core.admin.layertree.label.theme.opened"|trans}}">
            {{ form_widget(theme.opened) }}
        </span>
    </td>
</tr>
