{
    "name": "mapbender/mapbender",
    "description": "Mapbender library",
    "keywords": ["mapbender", "module"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=5.4.30",
        "symfony/symfony": "^2.4|^3",
        "symfony/security-acl": "~2.8 || ~3.0",
        "twig/extensions": "1.x",
        "doctrine/orm": "^2.2.3",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "sensio/generator-bundle": "^2 || ^3",

        "setasign/fpdi-fpdf": "1.6.*",

        "wheregroup/compass-mixins": "0.x",
        "mapbender/mapbender-icons": "1.x",

        "mapbender/vis-ui.js": "dev-master-keep-backward-compatibility as 0.2.83",
        "wheregroup/codemirror": "5.x",
        "components/underscore": "1.x",
        "wheregroup/open-sans": "1.x",
        "wheregroup/doctrine-dbal-shims": "^1",
        "wheregroup/assetic-filter-sassc": "^0.0.1",
        "wheregroup/sassc-binaries": "^0.0.1",

        "mapbender/fom": "~3.1.2 || ^3.2.1",
        "mapbender/owsproxy": "^3.0.6.2",

        "mapbender/mapquery": "1.x"
    },
    "autoload": {
        "psr-4": {"Mapbender\\": "src/Mapbender"}
    },
    "conflict": {
        "mapbender/digitizer": "<1.1.68",
        "mapbender/data-source": "<0.1.11",
        "mapbender/wmts": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-release/3.0.5": "3.0.5.x-dev",
            "dev-release/3.0.6": "3.0.6.x-dev",
            "dev-release/3.0.7": "3.0.8.x-dev"
        }
    }
}
